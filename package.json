{
  "name": "mssql-snapshot",
  "version": "1.1.0",
  "description": "An npm package for managing mssql snapshots.",
  "main": "./lib/mssql-snapshot.js",
  "scripts": {
    "test": "concurrently \"npm run test/unit\" \"npm run test/integration\"",
    "test/integration": "cross-env NODE_ENV=test mocha test/test-integration --compilers js:babel-core/register --require ./test-setup/testHelper.js --recursive --timeout 10000",
    "test/unit": "cross-env NODE_ENV=test mocha test/test-unit --compilers js:babel-core/register --require ./test-setup/testHelper.js --recursive",
    "clean": "rm -rf ./lib",
    "compile": "babel -d lib/ src/ && cp -r src/queries lib/queries",
    "watch": "babel -d -w src/ --out-dir lib/",
    "prepublish": "npm run clean && npm run compile && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeEmitter/mssql-snapshot.git"
  },
  "keywords": [
    "sql",
    "mssql",
    "snapshot",
    "backup"
  ],
  "author": "codeEmitter",
  "contributors": [
    "Andrew Smith <andrew@andrew.codes>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://trello.com/b/03NEIEfs/mssql-snapshot"
  },
  "homepage": "https://github.com/codeEmitter/mssql-snapshot#readme",
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-plugin-rewire": "1.1.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "concurrently": "3.5.0",
    "cross-env": "5.1.1",
    "mocha": "3.2.0",
    "sinon": "4.0.2",
    "sinon-chai": "2.14.0"
  },
  "dependencies": {
    "seriate": "git://github.com/versionone/seriate#D-13330_momentjs_vuln"
  }
}
